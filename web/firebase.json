{
    "hosting": {
        "public": "dist/public",
        "predeploy": "next build 'src/app/'",
        "rewrites": [
            {
                "source": "**/**",
                "function": "next"
            }
        ]
    },
    "functions": {
        "source": "dist/functions",
        "predeploy": "babel 'src/functions' --out-dir 'dist/functions'"
    }
}
