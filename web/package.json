        "grpc": "^1.22.2",
        "next": "^12.1.4",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-player": "2.10.0",
        "react-scroll": "^1.8.7",
        "react-transition-group": "^4.4.1",
        "soundcloud-v2-api": "^0.1.4"
