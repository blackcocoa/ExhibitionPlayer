{
    "name": "soundscraper-app",
    "version": "0.1.1",
    "description": "Audition app for M3",
    "engines": {
        "node": "14"
    },
    "scripts": {
        "dev": "next -p 3562 src/app/",
        "build": "next build src/app/",
        "deploy": "NODE_ENV=production firebase deploy",
        "emulators": "NODE_ENV=production firebase emulators:start",
        "clean": "rimraf \"dist/functions/**\" && rimraf dist/public",
        "install-deps": "cp src/functions/package.json dist/functions/package.json && cd dist/functions && yarn"
    },
    "dependencies": {
        "@babel/polyfill": "^7.10.4",
        "@material-ui/core": "^4.9.5",
        "@material-ui/icons": "^4.9.1",
        "@types/react-scroll": "^1.8.2",
        "axios": "^0.21.1",
        "firebase": "^8.3.3",
        "firebase-functions": "^3.13.2",
        "grpc": "^1.22.2",
        "next": "^10.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-player": "2.9.0",
        "react-scroll": "^1.8.1",
        "soundcloud-v2-api": "^0.1.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@google-cloud/logging": "^9.2.0",
        "@types/react": "^17.0.3",
        "@types/react-youtube": "^7.6.2",
        "cpx": "^1.5.0",
        "cross-env": "^7.0.3",
        "firebase-admin": "^9.6.0",
        "firebase-functions-test": "^0.2.0",
        "firebase-tools": "^9.9.0",
        "node-sass": "^5.0.0",
        "rimraf": "^3.0.2",
        "ts-loader": "^8.1.0",
        "typescript": "^4.2.4"
