        "grpc": "^1.22.2",
        "next": "^9.3.4",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-player": "2.6.0",
        "soundcloud-v2-api": "^0.1.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.4",
        "@babel/preset-env": "^7.11.0",
        "@google-cloud/logging": "^7.2.3",
        "@types/react": "^16.9.23",
        "@types/react-youtube": "^7.6.2",
        "cpx": "^1.5.0",
        "cross-env": "^5.2.0",
